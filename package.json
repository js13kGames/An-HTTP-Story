{
  "name": "remember-me",
  "version": "0.0.1",
  "description": "Entry for js13kGames - 2020 WebXR",
  "author": {
    "name": "Kai Zau",
    "email": "k@kaizau.com",
    "url": "https://kaizau.com"
  },
  "repository": "github:kaizau/remember-me",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "last 2 chrome versions, last 2 ff versions"
  ],
  "scripts": {
    "start": "webpack-dev-server --port 8888 --env.TARGET=local",
    "preview": "webpack-dev-server --port 8888 --env.TARGET=production",
    "build": "webpack --env.TARGET=production",
    "build-closure": "webpack --env.TARGET=production-closure",
    "analyze": "webpack --env.TARGET=analyze",
    "zip": "rimraf game.zip && zip -rj9 game.zip public && ls -l game.zip | awk '{print $5}'",
    "finalize": "webpack --env.TARGET=finalize && npm run zip",
    "finalize-closure": "webpack --env.TARGET=finalize-closure && npm run zip",
    "lint": "prettier --ignore-path .gitignore --write . && eslint --fix .",
    "test": "jest --watch"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "closure-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^6.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "google-closure-compiler": "^20200830.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.4.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
