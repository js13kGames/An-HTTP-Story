{
  "name": "a-http-story",
  "version": "0.0.1",
  "description": "Entry for js13kGames - 2020 WebXR",
  "author": {
    "name": "Kai Zau",
    "email": "k@kaizau.com",
    "url": "https://kaizau.com"
  },
  "repository": "github:kaizau/a-http-story",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "last 2 chrome versions, last 2 ff versions"
  ],
  "source": "site/index.js",
  "main": "public-micro/game.js",
  "scripts": {
    "start": "webpack-dev-server --port 8888 --env.TARGET=dev",
    "start-closure": "webpack --env.TARGET=build-closure && gulp compileDev",
    "start-closure-bundle": "webpack --env.TARGET=build-closure && gulp bundleDev",
    "micro": "microbundle --no-sourcemaps",
    "build": "webpack --env.TARGET=build",
    "build-terser": "webpack --env.TARGET=build-terser",
    "build-closure": "webpack --env.TARGET=build-closure",
    "build-closure-bundle": "webpack --env.TARGET=build-closure",
    "zip": "webpack --env.TARGET=finalize && gulp zipWebpack",
    "zip-terser": "webpack --env.TARGET=zip-terser && gulp zipTerser",
    "zip-closure": "webpack --env.TARGET=zip-closure && gulp compileProd && gulp zipClosure",
    "zip-closure-bundle": "webpack --env.TARGET=zip-closure && gulp bundleProd && gulp zipClosure",
    "preview": "webpack-dev-server --port 8888 --env.TARGET=build",
    "analyze": "webpack --env.TARGET=analyze",
    "lint": "prettier --ignore-path .gitignore --write . && eslint --fix .",
    "test": "jest --watch"
  },
  "devDependencies": {
    "browser-env": "^3.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "closure-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^6.1.0",
    "del": "^5.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "google-closure-compiler": "^20200830.0.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.4.1",
    "microbundle": "^0.12.3",
    "prettier": "^2.0.5",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "archiver": "^5.0.0"
  }
}
